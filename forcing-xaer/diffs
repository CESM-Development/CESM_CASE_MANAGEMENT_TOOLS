+r0 = 1188
+r1 = 1200
+r2 = 1211
+r3 = 3180
+r4 = 3191
+; copy variables
+if (.not.all(ismissing(in1_vars))) then
+do i = 0, dimsizes(in1_vars) - 1
+if (in1_vars(i) .eq. fixit) then
+	tmp  = in1->$in1_vars(i)$
+	nvar = tmp(r0:r2,:,:)
+	nvar = 0.
+	ctr=0
+        do r=0,1
+		ss = ctr
+		ee = ctr+11
+
+		nvar(ss:ee,:,:) = tmp(r1:r2,:,:)
+		ctr = ctr + 12
+
+        end do
+        nvar(23,:,:) = tmp(r2,:,:)
+else 
+   if (in1_vars(i) .eq. "date" .or. in1_vars(i) .eq. "time") then
+	tmp = in1->$in1_vars(i)$
+	time = in1->time
+	tvar = tmp(r0:r2)
+	tvar = 0
+        tvar(0:11)  = tmp(r0:r1-1)
+        ntime = new((/24/),typeof(time))
+	ntime(0:11) = time(r0:r1-1)
+	print(in1_vars(i))
+	; print(tmp(r1:r2))
+	; print(tmp(r3:r4))
+        tvar(12:23)  = tmp(r3:r4)
+	ntime(12:23) = time(r3:r4)
+	print(tvar)
+       delete([/tmp/])
+	print("tvar = " + tvar)
+	if(in1_vars(i) .eq. "time") then
+		delete(tvar)
+	end if
+else
+
+	fout->$in1_vars(i)$ = in1->$in1_vars(i)$
+end if
+end if
+
+end do
+end if
+
+
+ntime!0="time"
+ntime&time=ntime
+
+nvar&time = ntime
+tvar&time = ntime
