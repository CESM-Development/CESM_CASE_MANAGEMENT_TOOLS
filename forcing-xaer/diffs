+      tmp  = in1->$in1_vars(i)$
+      if (in1_vars(i) .eq. "date" ) then
+	tvar = tmp(r0:r2)
+	tvar = 0
+        tvar(0:11)  = tmp(r0:r1-1)
+        tvar(12:23)  = tmp(r3:r4)
+	print("tvar = " + tvar)
