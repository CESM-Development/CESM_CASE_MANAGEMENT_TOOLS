+;   20100915,   // date(69)
+;   20101015,   // date(70)
+;   20101115,   // date(71)
+;   20101215,   // date(72)
+
+
+r0=0
+r1=12
+r2=23
+r3=72
+r4=83
+; copy variables
+if (.not.all(ismissing(in1_vars))) then
+do i = 0, dimsizes(in1_vars) - 1
+if (in1_vars(i) .eq. fixit) then
+	tmp  = in1->$in1_vars(i)$
+	nvar = tmp(0:23,:,:,:)
+	nvar = 0.
+	ctr=0
+        do r=0,1
+		ss = ctr
+		ee = ctr+11
+
+		nvar(ss:ee,:,:,:) = tmp(r1:r2,:,:,:)
+		ctr = ctr + 12
+        end do
+        nvar(23,:,:,:) = tmp(r2,:,:,:)
+        delete([/tmp/])
+else if (in1_vars(i) .eq. "PS") then
+	tmp = in1->$in1_vars(i)$
+	wvar = tmp(r0:r2,:,:)
+	wvar = 0.
+        ctr=0
+        do r=0,1
+                ss = ctr
+                ee = ctr+11
+
+ printVarSummary(wvar)
+print(" ss == " + ss)
+print(" ee == " + ee)
+                wvar(ss:ee,:,:) = tmp(r1:r2,:,:)
+                ctr = ctr + 12
+        end do
+        wvar(23,:,:) = tmp(12,:,:)
+        fout->$in1_vars(i)$ = wvar
+        printMinMax(wvar,0)
+        delete([/wvar,tmp/])
+
+else 
+   if (in1_vars(i) .eq. "date" .or. in1_vars(i) .eq. "time") then
+        tmp  = in1->$in1_vars(i)$
+        time = in1->time
+        tvar = tmp(r0:r2)
+        tvar = 0
+        tvar(0:11)  = tmp(r0:r1-1)
+        ntime = new((/24/),typeof(time))
+        ntime(0:11) = time(r0:r1-1)
+        print(in1_vars(i))
+        tvar(12:23)  = tmp(r3:r4)
+        ntime(12:23) = time(r3:r4)
+        print(tvar)
+        print("tvar = " + tvar)
+        if(in1_vars(i) .eq. "time") then
+                delete(tvar)
+        end if
+        delete([/tmp/])
+
+else
+
+	fout->$in1_vars(i)$ = in1->$in1_vars(i)$
+end if
+end if
+end if
+
+
+end do
+end if
+
+ntime!0="time"
+ntime&time=ntime
+
+nvar&time = ntime
+tvar&time = ntime
+printVarSummary(nvar)
+fout->$fixit$ = nvar
+fout->date    = tvar
+;fout->time    = ntime
+
+delete([/tvar,nvar/])
+
+
+delete([/ifile,foutname,fout,in1_vars,in1_global/])
+
+end
